@article{whittaker_bedrock_2007,
	title = {Bedrock channel adjustment to tectonic forcing: {Implications} for predicting river incision rates},
	volume = {35},
	issn = {0091-7613},
	shorttitle = {Bedrock channel adjustment to tectonic forcing},
	url = {https://pubs.geoscienceworld.org/geology/article/35/2/103-106/129761},
	doi = {10.1130/G23106A.1},
	abstract = {We present detailed data of channel morphology for a river undergoing a transient response to active normal faulting where excellent constraints exist on spatial and temporal variations in fault slip rates. We show that traditional hydraulic scaling laws break down in this situation, and that channel widths become decoupled from drainage area upstream of the fault. Unit stream powers are {\textasciitilde}4 times higher than those predicted by current scaling paradigms and imply that incision rates for rivers responding to active tectonics may be signiﬁcantly higher than those heretofore modeled. The loss of hydraulic scaling cannot be explained by increasing channel roughness and is an intrinsic response to tectonic forcing. We show that channel aspect ratio is a strongly nonlinear function of local slope and demonstrate that fault-induced adjustment of channel geometries has reset hillslope gradients. The results give new insight into how rivers maintain their course in the face of tectonic uplift and illustrate the ﬁrst-order control the ﬂuvial system exerts on the locus and magnitude of sediment supply to basins.},
	language = {en},
	number = {2},
	urldate = {2023-05-09},
	journal = {Geology},
	author = {Whittaker, Alexander C. and Cowie, Patience A. and Attal, Mikaël and Tucker, Gregory E. and Roberts, Gerald P.},
	year = {2007},
	pages = {103},
	file = {Whittaker et al. - 2007 - Bedrock channel adjustment to tectonic forcing Im.pdf:/Users/vanessa/Zotero/storage/MEY2GJIZ/Whittaker et al. - 2007 - Bedrock channel adjustment to tectonic forcing Im.pdf:application/pdf},
}

@article{lave_fluvial_2001,
