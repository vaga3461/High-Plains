	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/esp.5200},
	keywords = {channel steepness, fluvial landscape, sediment-governed incision, Taiwan Coastal Range, tectonic geomorphology},
	pages = {2671--2689},
	file = {Full Text PDF:/Users/vanessa/Zotero/storage/ZMKRD4MG/Lai et al. - 2021 - Coarse sediment supply sets the slope of bedrock c.pdf:application/pdf;Snapshot:/Users/vanessa/Zotero/storage/S56M9T2J/esp.html:text/html},
